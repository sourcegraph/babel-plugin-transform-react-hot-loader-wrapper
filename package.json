{
  "name": "@sourcegraph/babel-plugin-transform-react-hot-loader-wrapper",
  "version": "0.0.0-DEVELOPMENT",
  "description": "Wraps all exported React components in a configurable set of files with react-hot-loader's hot() function",
  "keywords": [
    "babel-plugin",
    "react-hot-loader",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/babel-plugin-transform-react-hot-loader-wrapper"
  },
  "author": {
    "name": "Quinn Slack",
    "email": "sqs@sourcegraph.com"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "semantic-release": "semantic-release",
    "prettier": "prettier '**/*.{js?(on),ts?(x),scss,md,yml}' --write --list-different",
    "prettier-check": "npm run prettier -- --write=false",
    "eslint": "eslint './src/*.ts?(x)'",
    "build": "tsc -p .",
    "watch": "tsc -p . -w"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/helper-plugin-utils": "^7.13.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/helper-plugin-test-runner": "^7.1.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/traverse": "^7.4.4",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@sourcegraph/eslint-config": "^0.25.0",
    "@sourcegraph/prettierrc": "^3.0.3",
    "@sourcegraph/tsconfig": "^4.0.1",
    "@types/babel-plugin-tester": "9.0.3",
    "@types/babel__core": "7.1.14",
    "@types/babel__helper-plugin-utils": "7.10.0",
    "@types/babel__traverse": "7.14.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-tester": "^10.0.0",
    "eslint": "^7.30.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.3",
    "typescript": "^4.2.4"
  }
}
